    "NotificationQueue": "notifications"
  },
  "EventHub": {
    "ConnectionString": "YOUR_EVENT_HUB_CONNECTION_STRING",
    "EventHubName": "location-telemetry"
  },
  "Redis": {
    "ConnectionString": "YOUR_REDIS_CONNECTION_STRING"
  },
