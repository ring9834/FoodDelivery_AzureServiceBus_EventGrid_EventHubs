{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "CosmosDb": {
    "ConnectionString": "YOUR_COSMOS_DB_CONNECTION_STRING",
    "DatabaseName": "FoodDeliveryDb",
    "OrdersContainer": "Orders",
    "VendorsContainer": "Vendors",
    "DeliveryPersonsContainer": "DeliveryPersons",
    "CustomersContainer": "Customers"
  },
  "EventGrid": {
    "TopicEndpoint": "YOUR_EVENT_GRID_TOPIC_ENDPOINT",
    "AccessKey": "YOUR_EVENT_GRID_ACCESS_KEY"
  },
  "ServiceBus": {
    "ConnectionString": "YOUR_SERVICE_BUS_CONNECTION_STRING",
